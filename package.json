{
  "name": "teams-self-messaging",
  "version": "1.0.0",
  "description": "Microsoft Teams self-messaging service using Graph API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test-setup": "ts-node src/test-setup.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@azure/identity": "^3.4.2",
    "@azure/msal-node": "^3.5.3",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "axios": "^1.9.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "open": "^10.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
